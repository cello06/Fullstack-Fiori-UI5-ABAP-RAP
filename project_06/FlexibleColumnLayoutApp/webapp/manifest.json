{
	"_version": "1.38.0",
	"sap.app": {
		"id": "flexiblecolumnlayout",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "flexiblecolumnlayout.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.f": {},
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"config": {
			"fullWidth": true
		},"models": {
			"products" : {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "./model/Products.json"
			}
		}
	}
}