{
	"_version": "1.38.0",
	"sap.app": {
		"id": "flexiblecolumnlayout",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "flexiblecolumnlayout.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.f": {},
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"config": {
			"fullWidth": true
		},
		"models": {
			"products": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "./model/Products.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "flexiblecolumnlayout.view",
				"controlId": "fcl",
				"controlAggregation": "beginColumnPages",
				"transition": "slide",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": ["master"]
				},
				{
					"pattern": "detail/{productId}",
					"name": "detail",
					"target": ["master", "detail"]
				}
			],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"viewName": "Detail",
					"viewLevel": 2,
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}
